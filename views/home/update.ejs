<h1>Update Title</h1>
<form action="/post/<%= post.id %>" method="POST"> 
     <input type="hidden" name="_method" value="put" />
      Title <input id="title" type="text" name="title" value="<%= post.title %>" required>
      <br/>
      Content <textarea name="content" id="content" required><%= post.content %></textarea>
      <br/>
      <input type="submit" name="submit-btn" value="Submit">
</form>